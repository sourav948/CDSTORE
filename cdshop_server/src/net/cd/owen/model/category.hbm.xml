<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.cd.owen.model">
    <class name="Category" table="categories">
        <id column="categoryId" name="categoryId"> 
        <generator class="native"/> 
        </id>
        <property column="categoryName" name="categoryName" type="java.lang.String" />
        <property column="categoryDescription" name="categoryDescription" type="java.lang.String" />
    </class>
</hibernate-mapping>
